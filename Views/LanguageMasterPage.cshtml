@inherits UmbracoTemplatePage
@{
    Layout = null;
}
<!DOCTYPE html>
<!-- paulirish.com/2008/conditional-stylesheets-vs-css-hacks-answer-neither/ -->
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]> <html class="no-js lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]> <html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if gt IE 8]><!-->
<html lang="en">
<!--<![endif]-->
<head>
    <!-- TITLE -->
    <title>@CurrentPage.displayName | Bvlf.org</title>

    <!-- META -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">


    <!-- Included CSS Files -->
    <!-- Global CSS -->
    <link href="/Content/bootstrap.css" rel="stylesheet" />
    <!-- FONTS CSS-->
    <link href="/Content/fonts.css" rel="stylesheet" />
    <link href="/Content/font-awesome.css" rel="stylesheet" />
    <link href="/Content/SAP-icons.css" rel="stylesheet" />
    <!-- Plugins CSS -->
    <!-- ProFFF CSS-->
    <link href="/Content/Profff.css" rel="stylesheet" />


    <script type="text/javascript" src="/Scripts/jquery-3.1.1.min.js"></script>
    <script type="text/javascript" src="/Scripts/bootstrap.min.js"></script>
    @*<script type="text/javascript" src="/Scripts/jquery-ui.min.js"></script>*@
    @*<script type="text/javascript" src="/Scripts/fullcalendar.js"></script>*@

    <script src="http://maps.google.com/maps/api/js?key=AIzaSyBjE2-ZKbmGxeJ0u2tfwCA1KPwuMkhwbyo" async defer type="text/javascript"></script>
    <script src="https://code.jquery.com/jquery-2.1.4.min.js"></script>
    <script src="/Scripts/gmap3.min.js" type="text/javascript"></script>


</head>
<body>
    <div class="wrapper">
        @Html.Partial("_PublicHeader")

        <div>
            <div class="container">
                <div class="col-md-9" id="MainContent">
                    @RenderBody()
                </div>
                <div class="col-md-1" id="SpacerColumn">&nbsp;</div>
                <div class="col-md-3" id="RightColumn">
                    <div id="ContactUsHome">
                        @RenderSection("RightColumn", false)
                    </div>
                </div>
            </div>

        </div>
        <div id="Partners">
            <div class="container">
                @Html.Partial("~/Views/Partials/Sponsors.cshtml")
            </div>
        </div>

        @Html.Partial("Partials/_Footer")
        @RenderSection("cpFooter")

        <div id="FloorMat">
            <div class="container">
                <div class="row">
                    <div class="col-md-4" id="FooterLeft">
                        <p>
                            <a href="http://about.me/benoitgevaert" target="_blank"
                               tooltip="Created by Boenwas.net" alt="Created by Boenwas.net">
                                <img src="/Media/kolibri25b25.gif" tooltip="Created by Boenwas.net" alt="Created by Boenwas.net" />
                            </a>
                        </p>
                    </div>
                    <div class="col-md-4"></div>
                    <div class="col-md-offset-4" id="FooterRight">
                        <a href="/umbraco/umbraco.aspx" target="_blank" tooltip="Powered by Umbraco" alt="Powered by Umbraco">
                            <img src="/Media/Umbraco_25px.png" tooltip="Powered by Umbraco" alt="Powered by Umbraco" />
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- addthis settings-->
    <!-- Go to www.addthis.com/dashboard to customize your tools -->
    <script type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-580ce0a0c2f4f2cd"></script>
</body>

@RenderSection("cpScripts")

</html>